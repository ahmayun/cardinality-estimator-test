{
  "spark.range": {
    "type": "source",
    "parameters": {
      "start": {
        "type": "int",
        "required": true
      },
      "end": {
        "type": "int",
        "required": false
      },
      "step": {
        "type": "int",
        "required": false,
        "default": 1
      },
      "numSlices": {
        "type": "int",
        "required": false
      }
    },
    "returns": "RDD"
  },
  "as": {
    "type": "unary",
    "parameters": {
      "alias": {
        "type": "str",
        "required": true,
        "state-altering": true,
        "state-effect": "table-rename"
      }
    },
    "returns": "DataFrame"
  },
  "join": {
    "type": "binary",
    "parameters": {
      "other": {
        "type": "DataFrame",
        "required": true
      },
      "on": {
        "type": "Column",
        "required": true
      },
      "how": {
        "type": "str",
        "required": true,
        "default": "inner",
        "values": ["inner", "outer", "left", "right"]
      }
    },
    "returns": "DataFrame"
  },
  "withColumn": {
    "type": "unary",
    "parameters": {
      "colName": {
        "type": "str",
        "required": true,
        "state-altering": true,
        "state-effect": "column-add"
      },
      "col": {
        "type": "Column",
        "required": true
      }
    },
    "returns": "DataFrame"
  },
  "show": {
    "type": "action",
    "parameters": {
      "n": {
        "type": "int",
        "required": false,
        "default": 20
      },
      "truncate": {
        "type": "bool",
        "required": false,
        "default": true
      }
    },
    "returns": "None"
  }
}
